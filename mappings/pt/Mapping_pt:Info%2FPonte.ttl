        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/19> ,
			       <LatitudeMapping/1> ,
			       <LongitudeMapping/1> ,
			       <SimplePropertyMapping/16> ,
