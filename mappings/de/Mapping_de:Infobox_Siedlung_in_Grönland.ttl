### Main TriplesMap
####################
<http://de.dbpedia.org/resource/Mapping_de:Infobox_Siedlung_in_Grönland>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/10> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/6> ,
			       <SimplePropertyMapping/1> ,
			       <LatitudeMapping/0> ,
			       <SimplePropertyMapping/9> ,
			       <SimplePropertyMapping/12> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/8> ,
			       <SimplePropertyMapping/11> ,
			       <LongitudeMapping/0> ,
			       <SimplePropertyMapping/2> ,
			       <SimplePropertyMapping/7> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:Settlement ;
        rr:template  "http://de.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Infobox_Siedlung_in_Grönland" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://de.wikipedia.org/wiki/{wikititle}" .


