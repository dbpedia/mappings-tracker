        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/22> ,
			       <LatitudeMapping/0> ,
			       <SimplePropertyMapping/20> ,
			       <SimplePropertyMapping/27> ,
